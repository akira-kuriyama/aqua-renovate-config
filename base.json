{
   "regexManagers": [
      {
         "datasourceTemplate": "github-releases",
         "fileMatch": [
            "\\.?aqua\\.ya?ml"
         ],
         "matchStrings": [
            " +(?:version|'version'|\"version\") *: +['\"]?(?<currentValue>[^'\" \\n]+?)['\"]? +# renovate: depName=(?<depName>[^\\n]+)",
            " +(?:name|'name'|\"name\") *: +['\"]?(?<depName>[^'\" @/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*@(?<currentValue>[^'\" \\n]+)['\"]?"
         ]
      },
      {
         "datasourceTemplate": "github-releases",
         "fileMatch": [
            "\\.?aqua\\.ya?ml"
         ],
         "matchStrings": [
            " +(?:ref|'ref'|\"ref\") *: +['\"]?(?<currentValue>[^'\" \\n]+?)['\"]? +# renovate: depName=(?<depName>[^\\n]+)"
         ]
      }
   ]
}
