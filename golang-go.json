{
   "regexManagers": [
      {
         "datasourceTemplate": "github-tags",
         "depNameTemplate": "golang/go",
         "extractVersionTemplate": "^go(?<version>.*)$",
         "fileMatch": [
            "{{arg0}}"
         ],
         "matchStrings": [
            " +['\"]?version['\"]? *: +['\"]?(go)?(?<currentValue>[^'\" \\n]+)['\"]? +# renovate: depName=golang/go[ \\n]",
            " +['\"]?name['\"]? *: +['\"]?golang/go@(go)?(?<currentValue>[^'\" \\n]+)['\"]?"
         ]
      }
   ]
}
